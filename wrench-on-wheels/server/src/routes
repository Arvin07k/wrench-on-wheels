Sure, here's the proposed content for the file: /wrench-on-wheels/wrench-on-wheels/server/src/routes/index.ts

import { Router } from 'express';
import { createBooking, getBookings, updateBookingStatus } from '../controllers/bookingController';
import { registerMechanic, getMechanics } from '../controllers/mechanicController';
import { registerUser, loginUser } from '../controllers/authController';

const router = Router();

// User routes
router.post('/auth/register', registerUser);
router.post('/auth/login', loginUser);

// Booking routes
router.post('/bookings', createBooking);
router.get('/bookings', getBookings);
router.patch('/bookings/:id/status', updateBookingStatus);

// Mechanic routes
router.post('/mechanics/register', registerMechanic);
router.get('/mechanics', getMechanics);

export default router;